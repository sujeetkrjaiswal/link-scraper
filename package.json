{
  "name": "@utils-js/link-scraper",
  "version": "1.0.0-alpha-0.1",
  "description": "A command line utility to get all links of a URL, recursively to a given depth for whitelisted domains",
  "main": "bin/index.js",
  "engines": {
    "node": ">=11"
  },
  "bin": {
    "link-scraper": "bin/index.js"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc --skipLibCheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sujeetkrjaiswal/link-scraper.git"
  },
  "keywords": [
    "scraper",
    "link scraper",
    "scrape urls",
    "web url",
    "utility",
    "util"
  ],
  "author": "Sujeet Kumar Jaiswal)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sujeetkrjaiswal/link-scraper/issues"
  },
  "homepage": "https://github.com/sujeetkrjaiswal/link-scraper#readme",
  "dependencies": {
    "chalk": "^4.0.0",
    "cheerio": "^1.0.0-rc.3",
    "commander": "^5.1.0",
    "htmlparser2": "^4.1.0",
    "inquirer": "^7.1.0",
    "node-fetch": "^2.6.0",
    "ora": "^4.0.4"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/cheerio": "^0.22.18",
    "@types/commander": "^2.12.2",
    "@types/htmlparser2": "^3.10.1",
    "@types/inquirer": "^6.5.0",
    "@types/node-fetch": "^2.5.7",
    "@types/ora": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  }
